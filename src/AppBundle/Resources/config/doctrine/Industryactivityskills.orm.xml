<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Industryactivityskills" table="industryactivityskills">
    <indexes>
      <index name="IDX_industryActivitySkills_TID_AID" columns="blueprintTypeID,activityID"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="blueprinttypeid" type="integer" column="blueprintTypeID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activityid" type="boolean" column="activityID" nullable="true"/>
    <field name="skillid" type="integer" column="skillID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="level" type="boolean" column="level" nullable="true"/>
  </entity>
</doctrine-mapping>
